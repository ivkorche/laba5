#include <stdio.h> 
#include <stdlib.h> 

int main() 
{ 
int i = 0, array[10], evenCnt = 0, oddCnt = 0; 

for (i = 0; i < 10; i++) 
{ 
printf("enter %d number: ", i + 1); 
scanf_s("%d", &array[i]); 

if ((abs(array[i]) % 2) != 0) 
{ 
oddCnt++; 
} 
else 
{ 
evenCnt++; 
} 
} 

printf("\n"); 

if (oddCnt > evenCnt) 
{ 
for (i = 0; i < 10; i++) 
{ 
if ((abs(array[i]) % 2) == 0) 
{ 
array[i] = 0; 
} 

printf(" %d ", array[i]); 
} 
} 
else 
{ 
for (i = 0; i < 10; i++) 
{ 
if ((abs(array[i]) % 2) != 0) 
{ 
array[i] = 0; 
} 

printf(" %d ", array[i]); 
} 
} 

printf("\n press ENTER to continue\n"); 
getchar(); 
getchar(); 
return 0; 
}
